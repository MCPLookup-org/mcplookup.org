{
  "name": "mcplookup-registry",
  "version": "1.0.0",
  "description": "The One Ring MCP Server - Universal MCP Discovery Service with Flexible Storage Architecture",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "next build && tsc --project tsconfig.build.json",
    "start": "next start",
    "dev": "next dev",
    "dev:mcp": "tsx watch src/server.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:storage": "tsx scripts/test-storage.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --project tsconfig.typecheck.json",
    "openapi": "swagger-jsdoc -d swaggerDef.js src/pages/api/**/*.ts -o public/openapi.json",
    "deploy": "vercel --prod"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "discovery",
    "registry",
    "ai-tools",
    "serverless",
    "storage-abstraction",
    "redis",
    "upstash",
    "vercel",
    "dns-verification",
    "real-time-discovery",
    "typescript",
    "pagination",
    "error-handling"
  ],
  "author": "MCPLookup.org",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^3.19.2",
    "@emotion/react": "^11.14.0",
    "@esbuild/win32-x64": "^0.25.5",
    "@hookform/resolvers": "^3.9.1",
    "@modelcontextprotocol/sdk": "^1.12.1",
    "@types/prismjs": "^1.26.5",
    "@upstash/redis": "^1.35.0",
    "@vercel/mcp-adapter": "^0.7.3",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "framer-motion": "^12.16.0",
    "lucide-react": "^0.511.0",
    "mermaid": "^11.6.0",
    "next": "^15.3",
    "next-auth": "^5.0.0-beta.28",
    "next-themes": "^0.4.6",
    "node-fetch": "^3.3.2",
    "postcss": "^8.5.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.57.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "redis": "^5.5.5",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.22.0",
    "tailwind-merge": "^2.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "zod": "^3.25.50"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.29",
    "@types/pg": "^8.15.4",
    "@types/react": "19.1.6",
    "@types/redis": "^4.0.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/ui": "^3.2.1",
    "eslint": "^8.57.0",
    "happy-dom": "^17.6.3",
    "prettier": "^3.5.3",
    "prismjs": "^1.30.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.1"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "overrides": {
    "tree-sitter": "^0.21.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TSavo/mcplookup.org.git"
  },
  "bugs": {
    "url": "https://github.com/TSavo/mcplookup.org/issues"
  },
  "homepage": "https://mcplookup.org"
}
