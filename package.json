{
  "name": "mcplookup-registry",
  "version": "1.0.0",
  "description": "The One Ring MCP Server - Universal MCP Discovery Service",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "build": "next build && tsc",
    "start": "next start",
    "dev": "next dev",
    "dev:mcp": "tsx watch src/server.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "openapi": "swagger-jsdoc -d swaggerDef.js src/pages/api/**/*.ts -o public/openapi.json"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "discovery",
    "registry",
    "ai-tools",
    "streamable-http",
    "serverless",
    "vercel"
  ],
  "author": "MCPLookup.org",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "next": "^15.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8",
    "@hookform/resolvers": "^3.3.4",
    "react-hook-form": "^7.51.3",
    "tailwindcss": "^3.4.3",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "lucide-react": "^0.378.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.3.0",
    "node-fetch": "^3.3.1",
    "dns": "^0.2.2",
    "uuid": "^9.0.0",
    "dotenv": "^16.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.17.2"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/morgan": "^1.9.4",
    "@types/pg": "^8.10.1",
    "@types/uuid": "^9.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.7.1",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcplookup/registry.git"
  },
  "bugs": {
    "url": "https://github.com/mcplookup/registry/issues"
  },
  "homepage": "https://mcplookup.org"
}
